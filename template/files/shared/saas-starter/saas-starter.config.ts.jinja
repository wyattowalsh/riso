 * 
 * Generated: {{ "now" | strftime("%Y-%m-%d %H:%M:%S UTC") }}
 * Template Version: {{ _template | default("latest") }}
 */

export const SaaSStarterConfig = {
  meta: {
    version: "2025.11.02",
    generatedAt: "{{ "now" | strftime("%Y-%m-%dT%H:%M:%SZ") }}",
    templateVersion: "{{ _template | default("latest") }}",
    projectName: "{{ project_name }}",
    projectSlug: "{{ project_slug }}",
  },

  // Runtime Framework
  runtime: {
    id: "{{ saas_runtime }}",
    label: "{% if saas_runtime == 'nextjs-16' %}Next.js 16 (React 19.2, Turbopack){% else %}Remix 2.x (Vite, server-first routes){% endif %}",
    useWhen: "{% if saas_runtime == 'nextjs-16' %}Most SaaS, biggest ecosystem, Vercel-first{% else %}Complex forms, explicit data loading, edge-optimized{% endif %}",
  },

  // Hosting Platform
  hosting: {
    id: "{{ saas_hosting }}",
    label: "{% if saas_hosting == 'vercel' %}Vercel{% else %}Cloudflare Pages + Workers{% endif %}",
    useWhen: "{% if saas_hosting == 'vercel' %}Next-native, AI tooling, previews, edge, image, OG, Cron{% else %}Global edge, Workers, R2, lower egress, WebSockets{% endif %}",
  },

  // Database Provider
  database: {
    id: "{{ saas_database }}",
    label: "{% if saas_database == 'neon' %}Neon (Serverless Postgres){% else %}Supabase (Postgres + BaaS){% endif %}",
    useWhen: "{% if saas_database == 'neon' %}Database branching, autoscale-to-zero, fast cold starts{% else %}All-in-one platform, realtime, RLS, auth + storage{% endif %}",
  },

  // ORM
  orm: {
    id: "{{ saas_orm }}",
    label: "{% if saas_orm == 'prisma' %}Prisma{% else %}Drizzle{% endif %}",
    useWhen: "{% if saas_orm == 'prisma' %}Best TypeScript ergonomics, Prisma Studio, migration workflow{% else %}Lightweight, edge-optimized, SQL-like syntax{% endif %}",
  },

  // Authentication
  auth: {
    id: "{{ saas_auth }}",
    label: "{% if saas_auth == 'clerk' %}Clerk{% else %}Auth.js/NextAuth.js v5{% endif %}",
    useWhen: "{% if saas_auth == 'clerk' %}Hosted auth, built-in UI, organizations, passkeys, fastest setup{% else %}Self-hosted, OSS, full control, lower cost{% endif %}",
  },

  // Enterprise Auth Bridge
  enterpriseBridge: {
    id: "{{ saas_enterprise_bridge }}",
    label: "{% if saas_enterprise_bridge == 'workos' %}WorkOS (SSO + SCIM){% else %}None{% endif %}",
    useWhen: "{% if saas_enterprise_bridge == 'workos' %}Enterprise customers, SSO (Okta/Azure AD), SCIM provisioning{% else %}PLG motion, skip enterprise features{% endif %}",
  },

  // Billing Provider
  billing: {
    id: "{{ saas_billing }}",
    label: "{% if saas_billing == 'stripe' %}Stripe Billing 2025{% else %}Paddle{% endif %}",
    useWhen: "{% if saas_billing == 'stripe' %}Full control, usage metering, lower fees, manual tax{% else %}MoR, automatic tax/VAT, simpler compliance{% endif %}",
